
main :: () -> i32 {
    v1 := vector2.{ x: 32.0; y: 382.0; };
    v2 := vector2.{ x: 37.0; y: 38.0; };

    v3 := add(v1, v2);

    print v3.x;
    print v3.y;

    return 0;
};

vector2 :: struct {
    x := 0.0;
    y := 0.0;
};

add :: (a: vector2, b: vector2) -> vector2 {
    return .{ x: a.x + b.x; y: a.y + b.y; };
};




