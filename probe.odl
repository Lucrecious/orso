nest_t :: struct(key_t: type, fn: (key_t) -> key_t) {
    key := key_t.{};
    value :: fn;
};

main :: () -> void {
    add1 :: (n: int) -> int {
        return n + 1;
    };

    nest := nest_t(int, add1).{};
    printint(nest.key);
    println();

    x := nest_t(int, add1).value(10);
    printint(x);
    println();
};
