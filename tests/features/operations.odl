main :: () -> void {
    //int

    // scaler add
    {
        a := 1;
        b := 2;
        c := a + b;
        printint(a);
        printint(b);
        printint(c);
        println();
    };

    // scaler mult
    {
        a := 4;
        b := 5;
        c := a*b;
        printint(a);
        printint(b);
        printint(c);
        println();
    };

    // scaler sub
    {
        a := 10;
        b := 4;
        c := a - b;
        printint(a);
        printint(b);
        printint(c);
        println();
    };

    // scaler div
    {
        a := 100;
        b := 5;
        c := a / b;
        printint(a);
        printint(b);
        printint(c);
        println();
    };

    // scaler mod
    {
        a := -7;
        b := 5;
        c := a % b;
        printint(a);
        printint(b);
        printint(c);
        println();
    };

    // scaler rem
    {
        a := -7;
        b := 5;
        c := a %% b;
        printint(a);
        printint(b);
        printint(c);
        println();
    };
};