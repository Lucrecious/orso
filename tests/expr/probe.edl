{
    fib :: (n: i64) -> i64 {
        if n <= 0 then return 0;

        if n == 1 then return 1;

        a := 0;
        b := 1;

        return until n < 2 {
            n = n - 1;
            tmp := b;
            b = a + b;
            a = tmp;
        } else {
            b;
        };
    };

    result := fib(10);

    result;
}