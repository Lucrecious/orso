vector2 :: struct {
    x := 0;
    y := 0;
};

add :: (a: vector2, b: vector2) -> vector2 {
    sum: vector2;
    sum.x = a.x + b.x;
    sum.y = a.y + b.y;
    
    return sum;
};

main :: () -> i32 {
    a: vector2;
    b: vector2;

    a.x = 1;
    b.y = 1;

    sum := add(a, b);

    print sum.x;
    print sum.y;

    return 0;
};